<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="req_model_devolver_wizard">
        <field name="name">req.model.devolver.wizard</field>
        <field name="model">devolver.wizard</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="qty" />
                        <field name="accumulated_qty" invisible="1" />
                        <field name="line_id" invisible="1" />         
                    </group>
                    <group>
                                       
                    </group>
                </group> 
                <footer> 
                    <button name="devolver" string="Guardar" type="object" class="oe_highlight" />  
                    <button string="Cancel" special="cancel" class="oe_highlight"/>               
                </footer>
            </form>
        </field>
    </record>
</odoo>